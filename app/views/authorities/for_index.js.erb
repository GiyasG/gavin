$("#prjinator").on("click",
                  function(){
                    $("#prj").html("<%= escape_javascript render(@projects) %>")
                    $("#prjinator").html("<%= escape_javascript(paginate(@projects, remote: true)) %>")
                  });

  $("#pprinator").on("click",
                    function(){
                      $("#ppr").html("<%= escape_javascript render(@papers) %>")
                      $("#pprinator").html("<%= escape_javascript(paginate(@papers, remote: true)) %>")
                  });
